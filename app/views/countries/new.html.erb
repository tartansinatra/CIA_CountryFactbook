<%= form_for @currency do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :image %>
  <%= f.text_field :image %>
  <br>
  <%= f.label :size %>
  <%= f.integer :size %>
  <br>
  <%= f.label :population %>
  <%= f.integer :population %>
  <br>
  <%= f.label :language %>
  <%= f.text_field :language %>
  <br>
  <%= f.label :currency %>
  <%= f.text_field :currency %>
  <br>
  <%= f.submit 'Create' %>
  
<% end %>